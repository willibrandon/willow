{
  "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
  "name": "Willow Light",
  "author": "willibrandon",
  "themes": [
    {
      "name": "Willow Light",
      "appearance": "light",
      "style": {
        "background": "#ffffff",
        "foreground": "#333333",
        
        "comment": "#008000",
        "comment.documentation": "#008000",
        
        "comment.todo.keyword": {
          "color": "#0066cc",
          "font_weight": "bold"
        },
        "comment.todo.message": {
          "color": "#4a90e2",
          "font_style": "italic"
        },
        
        "comment.fixme.keyword": {
          "color": "#cc6600",
          "font_weight": "bold"
        },
        "comment.fixme.message": {
          "color": "#e6b800",
          "font_style": "italic"
        },
        
        "comment.hack.keyword": {
          "color": "#cc0000",
          "font_weight": "bold"
        },
        "comment.hack.message": {
          "color": "#e60000",
          "font_style": "italic"
        },
        
        "comment.note.keyword": {
          "color": "#009900",
          "font_weight": "bold"
        },
        "comment.note.message": {
          "color": "#00b300",
          "font_style": "italic"
        },
        
        "comment.bug.keyword": {
          "color": "#cc0066",
          "font_weight": "bold"
        },
        "comment.bug.message": {
          "color": "#e6007a",
          "font_style": "italic"
        },
        
        "comment.optimize.keyword": {
          "color": "#0066cc",
          "font_weight": "bold"
        },
        "comment.optimize.message": {
          "color": "#4a90e2",
          "font_style": "italic"
        },
        
        "comment.deprecated.keyword": {
          "color": "#666666",
          "font_weight": "bold",
          "font_style": "strikethrough"
        },
        "comment.deprecated.message": {
          "color": "#808080",
          "font_style": "strikethrough italic"
        },
        
        "comment.security.keyword": {
          "color": "#cc0000",
          "font_weight": "bold",
          "background_color": "#ffe6e6"
        },
        "comment.security.message": {
          "color": "#990000",
          "font_style": "italic",
          "background_color": "#fff0f0"
        },
        
        "comment.review.keyword": {
          "color": "#9932cc",
          "font_weight": "bold"
        },
        "comment.review.message": {
          "color": "#ba55d3",
          "font_style": "italic"
        },
        
        "comment.refactor.keyword": {
          "color": "#ff8c00",
          "font_weight": "bold"
        },
        "comment.refactor.message": {
          "color": "#ffa500",
          "font_style": "italic"
        },

        "text": "#333333",
        "text.literal": "#a31515",
        "text.uri": "#0066cc",

        "syntax": {
          "attribute": "#0066cc",
          "boolean": "#0066cc",
          "comment": "#008000",
          "constant": "#0066cc",
          "constructor": "#795e26",
          "emphasis": {
            "color": "#333333",
            "font_style": "italic"
          },
          "emphasis.strong": {
            "color": "#333333",
            "font_weight": "bold"
          },
          "function": "#795e26",
          "keyword": "#0066cc",
          "label": "#af00db",
          "link_text": "#0066cc",
          "link_uri": "#0066cc",
          "number": "#098658",
          "operator": "#333333",
          "property": "#001080",
          "punctuation": "#333333",
          "punctuation.bracket": "#0066cc",
          "punctuation.delimiter": "#333333",
          "punctuation.list_marker": "#0066cc",
          "string": "#a31515",
          "string.escape": "#ee0000",
          "string.regex": "#811f3f",
          "string.special": "#0066cc",
          "string.special.symbol": "#0066cc",
          "tag": "#0066cc",
          "type": "#267f99",
          "variable": "#001080",
          "variable.special": "#0066cc",
          "variant": "#267f99"
        },

        "players": [
          {
            "cursor": "#0066cc",
            "background": "#0066cc33",
            "selection": "#0066cc33"
          }
        ]
      }
    }
  ]
}
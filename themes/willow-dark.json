{
  "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
  "name": "Willow Dark",
  "author": "willibrandon",
  "themes": [
    {
      "name": "Willow Dark",
      "appearance": "dark",
      "style": {
        "background": "#1e1e1e",
        "foreground": "#d4d4d4",
        
        "comment": "#6a9955",
        "comment.documentation": "#6a9955",
        
        "comment.todo.keyword": {
          "color": "#4fc1ff",
          "font_weight": "bold"
        },
        "comment.todo.message": {
          "color": "#87ceeb",
          "font_style": "italic"
        },
        
        "comment.fixme.keyword": {
          "color": "#ffcc00",
          "font_weight": "bold"
        },
        "comment.fixme.message": {
          "color": "#ffd700",
          "font_style": "italic"
        },
        
        "comment.hack.keyword": {
          "color": "#ff6b6b",
          "font_weight": "bold"
        },
        "comment.hack.message": {
          "color": "#ff8a80",
          "font_style": "italic"
        },
        
        "comment.note.keyword": {
          "color": "#98c379",
          "font_weight": "bold"
        },
        "comment.note.message": {
          "color": "#90ee90",
          "font_style": "italic"
        },
        
        "comment.bug.keyword": {
          "color": "#e06c75",
          "font_weight": "bold"
        },
        "comment.bug.message": {
          "color": "#ff6b6b",
          "font_style": "italic"
        },
        
        "comment.optimize.keyword": {
          "color": "#61afef",
          "font_weight": "bold"
        },
        "comment.optimize.message": {
          "color": "#87ceeb",
          "font_style": "italic"
        },
        
        "comment.deprecated.keyword": {
          "color": "#7c7c7c",
          "font_weight": "bold",
          "font_style": "strikethrough"
        },
        "comment.deprecated.message": {
          "color": "#9c9c9c",
          "font_style": "strikethrough italic"
        },
        
        "comment.security.keyword": {
          "color": "#ff0000",
          "font_weight": "bold",
          "background_color": "#330000"
        },
        "comment.security.message": {
          "color": "#ff4444",
          "font_style": "italic",
          "background_color": "#220000"
        },
        
        "comment.review.keyword": {
          "color": "#dda0dd",
          "font_weight": "bold"
        },
        "comment.review.message": {
          "color": "#dda0dd",
          "font_style": "italic"
        },
        
        "comment.refactor.keyword": {
          "color": "#ffa500",
          "font_weight": "bold"
        },
        "comment.refactor.message": {
          "color": "#ffb347",
          "font_style": "italic"
        },

        "text": "#d4d4d4",
        "text.literal": "#ce9178",
        "text.uri": "#569cd6",

        "syntax": {
          "attribute": "#9cdcfe",
          "boolean": "#569cd6",
          "comment": "#6a9955",
          "constant": "#569cd6",
          "constructor": "#dcdcaa",
          "emphasis": {
            "color": "#d4d4d4",
            "font_style": "italic"
          },
          "emphasis.strong": {
            "color": "#d4d4d4",
            "font_weight": "bold"
          },
          "function": "#dcdcaa",
          "keyword": "#569cd6",
          "label": "#c586c0",
          "link_text": "#569cd6",
          "link_uri": "#569cd6",
          "number": "#b5cea8",
          "operator": "#d4d4d4",
          "property": "#9cdcfe",
          "punctuation": "#d4d4d4",
          "punctuation.bracket": "#ffd700",
          "punctuation.delimiter": "#d4d4d4",
          "punctuation.list_marker": "#6796e6",
          "string": "#ce9178",
          "string.escape": "#d7ba7d",
          "string.regex": "#d16969",
          "string.special": "#569cd6",
          "string.special.symbol": "#4fc1ff",
          "tag": "#569cd6",
          "type": "#4ec9b0",
          "variable": "#9cdcfe",
          "variable.special": "#569cd6",
          "variant": "#4ec9b0"
        },

        "players": [
          {
            "cursor": "#528bff",
            "background": "#528bff33",
            "selection": "#528bff33"
          }
        ]
      }
    }
  ]
}
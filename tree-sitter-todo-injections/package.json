{
  "name": "tree-sitter-todo-injections",
  "version": "1.0.0",
  "description": "Tree-sitter grammar for TODO/FIXME/HACK injection highlighting",
  "main": "bindings/node",
  "keywords": [
    "parser",
    "lexer",
    "todo",
    "fixme",
    "hack",
    "tree-sitter"
  ],
  "author": "willibrandon",
  "license": "MIT",
  "dependencies": {
    "nan": "^2.14.0"
  },
  "peerDependencies": {
    "tree-sitter": "^0.20.0"
  },
  "peerDependenciesMeta": {
    "tree-sitter": {
      "optional": true
    }
  },
  "devDependencies": {
    "tree-sitter-cli": "^0.20.0"
  },
  "tree-sitter": [
    {
      "scope": "comment.todo-injection",
      "file-types": [],
      "injection-regex": "todo-injection"
    }
  ]
}